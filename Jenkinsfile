pipeline {
	agent any

	stages {
    	stage("my-stage") {   
        	steps {               	 
            	echo 'hello-World'
        	}    
    	}
    	stage("my-stage2") {   
        	steps {               	 
            	echo 'hello World Again 2'
        	}    
    	}		

    	stage("my-stage3") {   
        	steps {               	 
            	echo 'hello-World Again 3'
        	}    
    	}
    	stage("Stage-4") {   
        	steps {               	 
            	echo 'hello World Again 4'
        	}    
    	}		
    	    	
}
}

